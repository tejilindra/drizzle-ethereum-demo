{
  "name": "reactstarter",
  "version": "1.0.0",
  "description": "drizzle-ethereum-dAPP-demo",
  "main": "index.js",
  "scripts": {
    "start": "node server.js",
    "start:dev": "webpack-dev-server --config ./webpackConfig/webpack.dev.js --color  --open",
    "build-prod": "webpack --config ./webpackConfig/webpack.prod.js",
    "format": "prettier --write \"**/*.+(js|jsx|json|yml|yaml|css|md|vue)\"",
    "clean": "rm -rf ./build ",
    "lint:fix": "eslint --fix --ext .js,.jsx .",
    "build": "npm run clean && npm run build-prod",
    "test": "jest --coverage --config ./jest.config.js",
    "test:watch": "jest --watch",
    "chromedriver": "chromedriver -v",
    "e2e": "nightwatch -e selenium.chrome e2e/index.js",
    "e2e-test": "npm run build-prod && concurrently -k --success first \"npm run start\" \"npm run e2e\""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react"
  ],
  "homepage": "",
  "dependencies": {
    "@drizzle/react-plugin": "^1.5.3",
    "@drizzle/store": "^1.5.3",
    "@material-ui/core": "^4.11.2",
    "@material-ui/data-grid": "^4.0.0-alpha.14",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "chromedriver": "^83.0.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.5.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "express": "^4.17.1",
    "formik": "^2.2.6",
    "framer-motion": "^3.1.1",
    "iedriver": "^3.14.2",
    "nightwatch": "^1.3.5",
    "path": "^0.12.7",
    "proptypes": "^1.1.0",
    "react": "^16.13.1",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.2.0",
    "selenium-server": "^3.141.59",
    "sinon": "^9.2.3",
    "yup": "^0.32.8"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.+(js|jsx)": [
      "eslint --fix --ext .js,.jsx .",
      "git add"
    ],
    "*.+(json|css|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.10.0",
    "@babel/plugin-transform-runtime": "^7.10.0",
    "@babel/preset-env": "^7.10.0",
    "@babel/preset-react": "^7.10.0",
    "@testing-library/jest-dom": "^5.11.8",
    "@testing-library/react": "^11.2.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "eslint": "^7.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "file-loader": "^6.0.0",
    "geckodriver": "^1.19.1",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.2.6",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "style-loader": "^1.2.1",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5",
    "webpack-dev-server": "^3.7.2",
    "webpack-merge": "^4.2.2"
  }
}
